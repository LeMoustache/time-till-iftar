[
    {
        "index": "1.",
        "name": "ALLAH c.c.",
        "text": "\u00d6z\u00fcnd\u0259 b\u00fct\u00fcn isim v\u0259 sif\u0259tl\u0259ri toplayan."
    },
    {
        "index": "2.",
        "name": "\u018fr-Rahman",
        "text": "M\u0259rh\u0259m\u0259tli davranan v\u0259 qoruyan."
    },
    {
        "index": "3.",
        "name": "\u018fr-Rahim",
        "text": "R\u0259hm ed\u0259n, ba\u011f\u0131\u015flayan."
    },
    {
        "index": "4.",
        "name": "\u018fl-M\u0259lik",
        "text": "M\u00fclk\u00fcn h\u0259qiqi sahibi, m\u00fclk v\u0259 s\u0259lt\u0259n\u0259ti davaml\u0131 olan."
    },
    {
        "index": "5.",
        "name": "\u018fl-Quddus",
        "text": "H\u0259r c\u00fcr q\u00fcsur v\u0259 eyibd\u0259n uzaq olan."
    },
    {
        "index": "6.",
        "name": "\u018fs-S\u0259lam",
        "text": "B\u0259nd\u0259l\u0259rini firavanl\u0131\u011fa \u00e7\u0131xaran."
    },
    {
        "index": "7.",
        "name": "\u018fl-Mumin",
        "text": "K\u00f6n\u00fcll\u0259rd\u0259 iman i\u015f\u0131\u011f\u0131 oyand\u0131ran."
    },
    {
        "index": "8.",
        "name": "\u018fl-Muh\u0259ymin",
        "text": "N\u0259zar\u0259t ed\u0259n v\u0259 qoruyan"
    },
    {
        "index": "9.",
        "name": "\u018fl-\u018fziz",
        "text": "M\u0259\u011flub edilm\u0259si \u0259sla m\u00fcmk\u00fcn olmayan."
    },
    {
        "index": "10.",
        "name": " \u018fl-C\u0259bbar",
        "text": "\u0130st\u0259diyini m\u00fctl\u0259q ed\u0259n, dil\u0259diyin\u0259 nail olan."
    },
    {
        "index": "11.",
        "name": " \u018fl-Mut\u0259k\u0259bbir",
        "text": "H\u0259r \u015feyd\u0259 b\u00f6y\u00fckl\u00fcy\u00fcn\u00fc g\u00f6st\u0259r\u0259n."
    },
    {
        "index": "12.",
        "name": " \u018fl-Xaliq",
        "text": "H\u0259r \u015feyi yoxdan var ed\u0259n."
    },
    {
        "index": "13.",
        "name": " \u018fl-B\u0259ri",
        "text": "H\u0259r \u015feyi uy\u011fun bir t\u0259rzd\u0259, bir birin\u0259 uy\u011fun yaradan."
    },
    {
        "index": "14.",
        "name": " \u018fl-Mus\u0259vvir",
        "text": "H\u0259r \u015fey\u0259 bir forma v\u0259 x\u00fcsusiyy\u0259t verib t\u0259svir ed\u0259n."
    },
    {
        "index": "15.",
        "name": " \u018fl-Qaffar",
        "text": "Qullar\u0131n\u0131n g\u00fcnahlar\u0131n\u0131 \u00f6rt\u0259n, g\u00fcnahlar\u0131n\u0131 ba\u011f\u0131\u015flayan"
    },
    {
        "index": "16.",
        "name": " \u018fl-K\u0259hhar",
        "text": "H\u0259r \u015fey\u0259 h\u0259r ist\u0259diyini ed\u0259 bil\u0259c\u0259k \u015f\u0259kild\u0259 qalib v\u0259 hakim olan."
    },
    {
        "index": "17.",
        "name": " \u018fl-Vahhab",
        "text": "H\u0259ddind\u0259n \u00e7ox b\u0259x\u015f ed\u0259n."
    },
    {
        "index": "18.",
        "name": " \u018fr-Razz\u0259q",
        "text": "B\u00fct\u00fcn m\u0259xluqat\u0131n ruzisini ver\u0259n v\u0259 ehtiyac\u0131n\u0131 qar\u015f\u0131layan."
    },
    {
        "index": "19.",
        "name": " \u018fl-F\u0259ttah",
        "text": "H\u0259r c\u00fcr \u00e7\u0259tinlikl\u0259ri asanla\u015fd\u0131ran, yoxsulluqdan qurtaran."
    },
    {
        "index": "20.",
        "name": " \u018fl-Alim",
        "text": "H\u0259r \u015feyi \u0259n ki\u00e7ik n\u00f6qt\u0259sin\u0259 q\u0259d\u0259r bil\u0259n, elmi \u0259b\u0259di v\u0259 \u0259z\u0259li olan."
    },
    {
        "index": "21.",
        "name": " \u018fl-Kabid",
        "text": "\u0130st\u0259diyin\u0259 yoxsulluq ver\u0259n, s\u0131xan, daraldan."
    },
    {
        "index": "22.",
        "name": " \u018fl-Basit",
        "text": "\u0130st\u0259diyin\u0259 bolluq ver\u0259n, a\u00e7an, geni\u015fl\u0259d\u0259n."
    },
    {
        "index": "23.",
        "name": " \u018fl-Hafid",
        "text": "Yuxar\u0131dan a\u015fa\u011f\u0131ya endir\u0259n, al\u00e7aldan."
    },
    {
        "index": "24.",
        "name": " \u018fr-Rafi",
        "text": "Yuxar\u0131 qald\u0131ran, y\u00fcks\u0259ld\u0259n."
    },
    {
        "index": "25.",
        "name": " \u018fl-Muiz",
        "text": "\u0130zz\u0259t verib qonaq kimi q\u0259bul ed\u0259n."
    },
    {
        "index": "26.",
        "name": " \u018fl-Muzil",
        "text": "Danlayaraq z\u0259if v\u0259 d\u0259y\u0259rsiz ed\u0259n."
    },
    {
        "index": "27.",
        "name": " \u018fl-S\u0259mi",
        "text": "H\u0259r \u015feyi e\u015fid\u0259n."
    },
    {
        "index": "28.",
        "name": " \u018fl-B\u0259sir",
        "text": "H\u0259r \u015feyi g\u00f6r\u0259n."
    },
    {
        "index": "29.",
        "name": " \u018fl-H\u0259k\u0259m",
        "text": "Hikm\u0259t sahibi olan, etdiyi h\u0259r i\u015fd\u0259 hikm\u0259tin olmas\u0131na n\u0259zar\u0259t ed\u0259n."
    },
    {
        "index": "30.",
        "name": " \u018fl-\u018fdl",
        "text": "H\u0259ddind\u0259n \u00e7ox \u0259dal\u0259tli olan."
    },
    {
        "index": "31.",
        "name": " \u018fl-L\u0259tif",
        "text": "\u0130\u015fl\u0259rin b\u00fct\u00fcn inc\u0259likl\u0259rini bil\u0259n."
    },
    {
        "index": "32.",
        "name": " \u018fl-Xabir",
        "text": "H\u0259r \u015feyin i\u00e7 \u00fcz\u00fcnd\u0259n, gizli t\u0259r\u0259find\u0259n x\u0259b\u0259rdar olan."
    },
    {
        "index": "33.",
        "name": " \u018fl-H\u0259lim",
        "text": "Yum\u015faq davranan, m\u00fclayim olan."
    },
    {
        "index": "34.",
        "name": " \u018fl-\u018fzim",
        "text": "\u00c7ox \u0259z\u0259m\u0259tli v\u0259 b\u00f6y\u00fck olan."
    },
    {
        "index": "35.",
        "name": " \u018fl-Qafur",
        "text": "\u00c7ox ba\u011f\u0131\u015flayan, \u0259fvi \u00e7ox olan."
    },
    {
        "index": "36.",
        "name": " \u018fl-\u015e\u0259kur",
        "text": "\u00d6z raz\u0131l\u0131\u011f\u0131 \u00fc\u00e7\u00fcn edil\u0259n yax\u015f\u0131 i\u015fl\u0259ri y\u00fcks\u0259k d\u0259r\u0259c\u0259d\u0259 m\u00fckafatland\u0131ran."
    },
    {
        "index": "37.",
        "name": " \u018fl-\u018fliyy",
        "text": "\u00c7ok uca. \u00c7ox y\u00fcks\u0259k olan"
    },
    {
        "index": "38.",
        "name": " \u018fl-K\u0259bir",
        "text": "B\u00f6y\u00fckl\u00fcy\u00fcn\u00fcn h\u00fcdudu olmayan."
    },
    {
        "index": "39.",
        "name": " \u018fl-Hafiz",
        "text": "Edil\u0259n \u0259m\u0259ll\u0259ri b\u00fct\u00fcn t\u0259fsilat\u0131yla, \u0259n ki\u00e7ik x\u0131rdal\u0131\u011f\u0131na kimi qeyd ed\u0259n."
    },
    {
        "index": "40.",
        "name": " \u018fl-Mukit",
        "text": "Yarad\u0131lm\u0131\u015f h\u0259r \u015feyin qidas\u0131n\u0131 ver\u0259n."
    },
    {
        "index": "41.",
        "name": " \u018fl-H\u0259sib",
        "text": "H\u0259r k\u0259sin h\u0259yat\u0131 boyunca ettiyi \u0259m\u0259ll\u0259ri sor\u011fulayan."
    },
    {
        "index": "42.",
        "name": " \u018fl-C\u0259lil",
        "text": "\u018fz\u0259m\u0259t v\u0259 ululuk sahibi olan."
    },
    {
        "index": "43.",
        "name": " \u018fl-K\u0259rim",
        "text": "\u00c7ox b\u0259x\u015f ed\u0259n. K\u0259r\u0259mi v\u0259 \u0259fvi bol olan."
    },
    {
        "index": "44.",
        "name": " \u018fr-Raqib",
        "text": "B\u00fct\u00fcn varl\u0131qlar \u00fcz\u0259rind\u0259 g\u00fcc\u00fc olan."
    },
    {
        "index": "45.",
        "name": " \u018fl-Mucib",
        "text": "Dua ed\u0259nl\u0259rin dualar\u0131n\u0131 q\u0259bul ed\u0259n, ist\u0259kl\u0259rini ver\u0259n."
    },
    {
        "index": "46.",
        "name": " \u018fl-Vasi",
        "text": "L\u00fctf\u00fc bol olan."
    },
    {
        "index": "47.",
        "name": " \u018fl-H\u0259kim",
        "text": "\u018fmrl\u0259ri, k\u0259lamlar\u0131 v\u0259 b\u00fct\u00fcn i\u015fl\u0259ri hikm\u0259tli olan."
    },
    {
        "index": "48.",
        "name": " \u018fl-V\u0259dud",
        "text": "Yax\u015f\u0131 qullar\u0131n\u0131 sev\u0259n, sevilm\u0259y\u0259 v\u0259 dostlu\u011fa haqq\u0131yla layiq olan."
    },
    {
        "index": "49.",
        "name": " \u018fl-M\u0259cid",
        "text": "\u015ean\u0131 \u00e7ox b\u00f6y\u00fck v\u0259 \u00e7ox y\u00fcks\u0259k olan."
    },
    {
        "index": "50.",
        "name": " \u018fl-Bais",
        "text": "\u00d6l\u00fcl\u0259ri dirildib q\u0259birl\u0259rd\u0259n \u00e7\u0131xaran."
    },
    {
        "index": "51.",
        "name": " \u018f\u015f-\u015e\u0259hid",
        "text": "H\u0259r zaman v\u0259 h\u0259r yerd\u0259 haz\u0131r v\u0259 nazir olan."
    },
    {
        "index": "52.",
        "name": " El-Hakk",
        "text": "Vacib'ul v\u00fccud olan, varl\u0131\u011f\u0131 he\u00e7 d\u0259yi\u015fm\u0259d\u0259n duran."
    },
    {
        "index": "53.",
        "name": " \u018fl-V\u0259kil",
        "text": "\u0130\u015fl\u0259rini ona \u0259man\u0259t ed\u0259nl\u0259rin i\u015fini d\u00fcz\u0259ld\u0259n v\u0259 h\u0259r \u015feyin yax\u015f\u0131s\u0131n\u0131 t\u0259min ed\u0259n."
    },
    {
        "index": "54.",
        "name": " \u018fl-Qaviyy",
        "text": "\u00c7ox q\u00fcvv\u0259tli. \u00c7ox g\u00fc\u00e7l\u00fc olan."
    },
    {
        "index": "55.",
        "name": " \u018fl-M\u0259tin",
        "text": "\u00c7ox sa\u011flam olan."
    },
    {
        "index": "56.",
        "name": " \u018fl-V\u0259liyy",
        "text": "Yax\u015f\u0131 qullar\u0131na, h\u0259qiqi m\u00f6minl\u0259r\u0259 dost olan."
    },
    {
        "index": "57.",
        "name": " \u018fl-H\u0259mid",
        "text": "H\u0259r c\u00fcr h\u0259md v\u0259 t\u0259rif\u0259 layiq olan."
    },
    {
        "index": "58.",
        "name": " \u018fl-M\u00fchsi",
        "text": "\u0130stisnas\u0131z h\u0259r \u015feyin t\u0259k t\u0259k say\u0131n\u0131 bil\u0259n."
    },
    {
        "index": "59.",
        "name": " \u018fl-M\u00fcbdi",
        "text": "B\u00fct\u00fcn m\u0259xluqat\u0131 madd\u0259siz v\u0259 n\u00fcmun\u0259siz ilk ba\u015fdan yaradan."
    },
    {
        "index": "60.",
        "name": " \u018fl-Muid",
        "text": "Yarada\u0131lm\u0131\u015flar\u0131 yox etdikd\u0259n sonra tekrar yaradan."
    },
    {
        "index": "61.",
        "name": " \u018fl-Muhyi",
        "text": "Canland\u0131ran, dirild\u0259n, can ba\u011f\u0131\u015flayan, sa\u011fl\u0131q ver\u0259n."
    },
    {
        "index": "62.",
        "name": " \u018fl-M\u00fcmit",
        "text": "\u00d6l\u00fcm\u00fc yaradan, \u00f6ld\u00fcr\u0259n."
    },
    {
        "index": "63.",
        "name": " \u018fl-H\u0259yy",
        "text": "Diri, tam v\u0259 m\u00fck\u0259mm\u0259l m\u0259nas\u0131yla h\u0259yat sahibi."
    },
    {
        "index": "64.",
        "name": " \u018fl-Qayyum",
        "text": "G\u00f6yl\u0259ri, yeri v\u0259 h\u0259r \u015feyi tutan."
    },
    {
        "index": "65.",
        "name": " \u018fl-Vacid",
        "text": "\u0130st\u0259diyini ist\u0259diyi vaxtda tapan."
    },
    {
        "index": "66.",
        "name": " \u018fl-M\u0259cid",
        "text": "S\u0259xav\u0259ti b\u00f6y\u00fck, k\u0259r\u0259mi bol olan."
    },
    {
        "index": "67.",
        "name": " \u018fl-Vahid",
        "text": "T\u0259k olan."
    },
    {
        "index": "68.",
        "name": " \u018fs-Sam\u0259d",
        "text": "S\u0131\u011f\u0131nacaq t\u0259k dayaq olan."
    },
    {
        "index": "69.",
        "name": " \u018fl-Qadir",
        "text": "H\u0259r \u015fey\u0259 g\u00fcc\u00fc \u00e7atan, h\u0259r ist\u0259diyini etm\u0259y\u0259 qadir olan."
    },
    {
        "index": "70.",
        "name": " \u018fl-Muqt\u0259dir",
        "text": "Q\u00fcvv\u0259t v\u0259 q\u00fcdr\u0259t sahibl\u0259ri \u00fcz\u0259rind\u0259 ist\u0259diyi kimi q\u0259na\u0259t ed\u0259n."
    },
    {
        "index": "71.",
        "name": " \u018fl-Muqaddim",
        "text": "\u0130st\u0259diyini ir\u0259li \u00e7\u0259kib, \u00f6n\u0259 alan."
    },
    {
        "index": "72.",
        "name": " \u018fl-M\u00fc\u0259xxir",
        "text": "\u0130st\u0259diyini geri at\u0131b, arxada buraxan."
    },
    {
        "index": "73.",
        "name": " \u018fl-\u018fvv\u0259l",
        "text": "Ba\u015flan\u011f\u0131c\u0131 olmayan, ilk olan."
    },
    {
        "index": "74.",
        "name": " \u018fl-Axir",
        "text": "Biti\u015fi olmayan, son olan."
    },
    {
        "index": "75.",
        "name": " \u018fz-Zahir",
        "text": "A\u00e7\u0131qca bilin\u0259n, a\u015fkar olan."
    },
    {
        "index": "76.",
        "name": " \u018fl-Batin",
        "text": "Gizli olan."
    },
    {
        "index": "77.",
        "name": " \u018fl-Vali",
        "text": "H\u0259r \u015feyi t\u0259k ba\u015f\u0131na idar\u0259 ed\u0259n."
    },
    {
        "index": "78.",
        "name": " \u018fl-Mut\u0259ali",
        "text": "A\u011fl\u0131n m\u00fcmk\u00fcn g\u00f6rd\u00fcy\u00fc h\u0259r \u015feyd\u0259n, h\u0259r hal v\u0259 m\u00fcnasib\u0259td\u0259n azad olan."
    },
    {
        "index": "79.",
        "name": " \u018fl-B\u0259rr",
        "text": "Qullar\u0131na yax\u015f\u0131l\u0131\u011f\u0131 v\u0259 ehsan\u0131, nem\u0259tl\u0259ri bol olan."
    },
    {
        "index": "80.",
        "name": " \u018ft-T\u0259vvab",
        "text": "T\u00f6vb\u0259l\u0259ri q\u0259bul ed\u0259n."
    },
    {
        "index": "81.",
        "name": " \u018fl-Munt\u0259kim",
        "text": "G\u00fcnahkarlar\u0131 \u0259dal\u0259tiyl\u0259 c\u0259zaland\u0131r\u0131b intiqam alan."
    },
    {
        "index": "82.",
        "name": " \u018fl-\u018ffuv",
        "text": "\u018ffvi v\u0259 r\u0259hm\u0259ti \u00e7ox olan, ba\u011f\u0131\u015flayan."
    },
    {
        "index": "83.",
        "name": " \u018fr-Rauf",
        "text": "Qullar\u0131na r\u0259hm ed\u0259n, onlar\u0131 l\u00fctf v\u0259 m\u0259rh\u0259m\u0259tl\u0259 qoruyan."
    },
    {
        "index": "84.",
        "name": " Malik\u00fc\u2019l-M\u00fclk",
        "text": "M\u00fclk\u00fcn \u0259b\u0259di sahibi olan."
    },
    {
        "index": "85.",
        "name": " Z\u00fc\u2019l- C\u0259lal-i v\u0259\u2019l-\u0130kram",
        "text": "H\u0259r c\u00fcr b\u00f6y\u00fckl\u00fcy\u00fcn, h\u0259r c\u00fcr k\u0259r\u0259min sahibi olan."
    },
    {
        "index": "86.",
        "name": " \u018fl-Muqsit",
        "text": "B\u00fct\u00fcn i\u015fl\u0259ri bir-birin\u0259 uy\u011fun ed\u0259n."
    },
    {
        "index": "87.",
        "name": " \u018fl-Cami",
        "text": "\u0130st\u0259diyini ist\u0259diyi zaman ist\u0259diyi yerd\u0259 toplayan."
    },
    {
        "index": "88.",
        "name": " \u018fl-Qaniyy",
        "text": "\u00c7ox varl\u0131 olan."
    },
    {
        "index": "89.",
        "name": " \u018fl-Mu\u011fni",
        "text": "\u0130st\u0259diyini z\u0259ngin ed\u0259n."
    },
    {
        "index": "90.",
        "name": " \u018fl-Mani",
        "text": "B\u0259zi \u015feyl\u0259rin meydana g\u0259lm\u0259sin\u0259 icaz\u0259 verm\u0259y\u0259n, \u0259ng\u0259ll\u0259y\u0259n."
    },
    {
        "index": "91.",
        "name": " \u018fd-Dar",
        "text": "D\u0259rd v\u0259 z\u0259r\u0259r ver\u0259c\u0259k \u015feyl\u0259ri yaradan, m\u0259yus ed\u0259n."
    },
    {
        "index": "92.",
        "name": " \u018fl-Nafi",
        "text": "Xeyir v\u0259 m\u0259nf\u0259\u0259t ver\u0259c\u0259k \u015feyl\u0259ri yaradan."
    },
    {
        "index": "93.",
        "name": " \u018fl-Nur",
        "text": "Al\u0259ml\u0259ri nurland\u0131ran."
    },
    {
        "index": "94.",
        "name": " \u018fl-Hadi",
        "text": "Hiday\u0259t\u0259 v\u0259 do\u011fru yola \u00e7atd\u0131ran."
    },
    {
        "index": "95.",
        "name": " \u018fl-B\u0259di",
        "text": "T\u0259krar\u0131 olmayan, misilsiz, heyr\u0259tverici ne\u00e7\u0259-ne\u00e7\u0259 al\u0259ml\u0259r yaradan."
    },
    {
        "index": "96.",
        "name": " \u018fl-Baki",
        "text": "Varl\u0131\u011f\u0131n\u0131n sonu olmayan, \u0259b\u0259di olan."
    },
    {
        "index": "97.",
        "name": " \u018fl-Varis",
        "text": "S\u0259rv\u0259tl\u0259rin h\u0259qiqi sahibi"
    },
    {
        "index": "98.",
        "name": " \u018fr-R\u0259\u015fid",
        "text": "B\u00fct\u00fcn i\u015fl\u0259ri \u0259z\u0259li niyy\u0259tin\u0259 uy\u011fun bir nizam v\u0259 hikm\u0259tl\u0259 sona q\u0259d\u0259r \u00e7atd\u0131ran."
    },
    {
        "index": "99.",
        "name": " \u018fs-Sab\u00fbr",
        "text": "\u00c7ox s\u0259birli olan."
    },
    {
        "index": "99.",
        "name": " \u0410\u0441-\u0421\u0430\u0431\u0443\u0440",
        "text": "\u041a\u0442\u043e \u043e\u0447\u0435\u043d\u044c \u0442\u0435\u0440\u043f\u0435\u043b\u0438\u0432."
    }
]